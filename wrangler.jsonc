{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cf-feedback-agent",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": [
			"0 17 * * *"
		]
	},
	"ai": {
		"binding": "AI"
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "feedback-db2",
			"database_id": "3be3be32-7a14-4b2d-9332-e6fe758c777d"
		}
	]
}